{
  "name": "api-types",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/api-types",
  "tags": ["scope:shared", "type:types"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/api-types",
        "main": "libs/shared/api-types/index.ts",
        "tsConfig": "libs/shared/api-types/tsconfig.lib.json",
        "assets": ["libs/shared/api-types/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/api-types/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
